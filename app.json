{
  "expo": {
    "name": "react-native-proyect",
    "slug": "e-commerce-React-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconft.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.coderhouse.ecommerce",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions":
      [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTENAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
